	<body>
		<div id="content">
			<div id="mktg">
				<div id="topbar">
					<div id="nav" class="page-wrap">
					<div class="right">
						<ul>
							<li><%= link_to "Home", "http://wikidlabs.com/reveille/"%></li>
							<li><%= link_to "Blog", "http://wikidlabs.com/blog/"%></li>
							<li><%= link_to "Crafted by WikidLabs", "http://wikidlabs.com"%></li>
						</ul>
					</div>
					<div class="clear"></div>
					</div>
				</div>

			<%= yield %>
			
			<div id="footer">
				<div id="foot" class="page-wrap">
					<div>
						<ul>
							<li><%= link_to "Home", "http://wikidlabs.com/reveille/"%></li>
							<li><%= link_to "Blog", "http://wikidlabs.com/blog/"%></li>
							<li><%= link_to "Send us an email", "mailto:hello@wikidlabs.com"%></li>
